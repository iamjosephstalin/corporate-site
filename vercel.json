{
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "shichifukutekx.com"
                }
            ],
            "destination": "https://www.shichifukutekx.com/:path*",
            "permanent": true
        }
    ],
    "headers": [
        {
            "source": "/(.*)\\.(jpg|jpeg|png|webp|svg|css|js|woff|woff2|ico)$",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}